{
  "name": "afterglow-access",
  "version": "1.0.12",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.config.json",
    "start-es": "ng serve --configuration=development,es --proxy-config proxy.config.json",
    "build": "ng build --configuration production",
    "patch-version": "npm --no-git-tag-version version patch && node ./patch-version.js",
    "i18n": "ng extract-i18n --format=xmb --out-file src/locale/messages.xmb",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix lint",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\""
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.4",
    "@angular/cdk": "^13.2.4",
    "@angular/common": "~13.2.4",
    "@angular/compiler": "~13.2.4",
    "@angular/core": "~13.2.4",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~13.2.4",
    "@angular/material": "^13.2.4",
    "@angular/platform-browser": "~13.2.4",
    "@angular/platform-browser-dynamic": "~13.2.4",
    "@angular/router": "~13.2.4",
    "@highlightjs/cdn-assets": "^11.5.1",
    "@ngxs-labs/immer-adapter": "^3.0.5",
    "@ngxs/logger-plugin": "^3.7.1",
    "@ngxs/router-plugin": "^3.7.1",
    "@ngxs/store": "^3.7.1",
    "@swimlane/ngx-charts": "^17.0.1",
    "angular-plotly.js": "^3.0.0",
    "angular2-uuid": "^1.1.1",
    "bootstrap-css-only": "^4.4.1",
    "camelcase-keys": "^6.2.2",
    "deepmerge": "^4.2.2",
    "everpolate": "0.0.3",
    "fast-deep-equal": "^3.1.3",
    "file-saver": "^2.0.5",
    "font-awesome": "^4.7.0",
    "immer": "^8.0.1",
    "jstat": "^1.9.4",
    "jwt-decode": "^3.1.2",
    "ml-levenberg-marquardt": "^4.0.0",
    "moment": "^2.29.1",
    "ng-keyboard-shortcuts": "^10.1.17",
    "ngx-avatar": "^4.0.0",
    "ngx-color-picker": "^12.0.0",
    "ngx-cookie": "^5.0.2",
    "ngx-highlight-js": "^13.0.0",
    "ngx-json-viewer": "^3.0.2",
    "ngx-papaparse": "^5.0.0",
    "normalize-wheel": "^1.0.1",
    "paper": "^0.12.11",
