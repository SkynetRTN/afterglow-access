    "query-string": "^6.14.1",
    "rxjs": "~6.6.0",
    "snakecase-keys": "^3.2.1",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^13.1.0",
    "@angular-devkit/build-angular": "~13.2.5",
    "@angular/cli": "~13.2.5",
    "@angular/compiler-cli": "~13.2.4",
    "@angular/localize": "^13.2.4",
    "@types/jasmine": "~3.6.0",
